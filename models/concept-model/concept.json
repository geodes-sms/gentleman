                                "constraint": {
                                    "values":  ["value"]
                                }
                            },
                            {
                                "name": "type",
                                "rules": [
                                    { 
                                        "type": "eq", 
                                        "eq": [{ "type": "attribute", "attribute": "property" }, "value"],
                                        "value": ["constraint-number-value", "constraint-number-range"]
                                    }
                                ]
                            }
                        ]
                    }
                }
            ],
            "properties": [
                { "name": "cname", "type": "string", "value": "number" }
            ]
        },
        {
            "name": "boolean primitive",
            "nature": "concrete",
            "prototype": "primitive",
            "attributes": [
